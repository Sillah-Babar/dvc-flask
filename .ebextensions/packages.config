packages:
  yum:
    python3-devel: []
  commands:
    copy_files:
      command: sudo cp -ra  /var/app/current/api/* /var/app/current/
    install_requirements:
      command: pip install -r /var/app/current/requirements.txt
    pull_weights_from_s3:
      command: cd var/app/current
      command: dvc pull

 
